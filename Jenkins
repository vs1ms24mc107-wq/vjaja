pipeline
{
  agent any
  
    stages
    {
        stage('build')
        {
            steps
            {
                git branch: "main", url:"https://github.com/vs1ms24mc107-wq/vjaja.git"
            }
        }
        stage('configure')
        {
            steps
            {
                echo "Configuring the file"
                sh "javac one.java"
            }
        }
        stage('run')
        {
            steps
            {
                echo "Running the file"
                sh "java one"
            }
        }
    }
}
